<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="AppTheme" parent="@style/AppTheme.Base" tools:ignore="ResourceCycle">
    </style>

    <style name="AppTheme.Base" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="android:textColorSecondary">@color/listitem</item>
        <item name="android:textColor">@color/secondrytext</item>
        <item name="android:textAllCaps">false</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="colorControlHighlight">@color/colorHighlight</item>
        <item name="colorPrimary">@color/appcolor</item>
        <item name="colorPrimaryDark">@color/appcolor</item>
    </style>
    <style name="CustomDialog" parent="@style/Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">#3f51b5</item>
    </style>
</resources>